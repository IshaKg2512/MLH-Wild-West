{
    "manifest_version": 3,
    "name": "Wild West",
    "version": "1.0",
    "icons": { "16": "icon16.png",
           "64": "icon64.png",
          "128": "icon128.png" },
    "content_scripts": [
        {

    
            "matches": ["<all_urls>"],
            "js": ["extra.js", "content.js"]
        }
    ],
    
    "host_permissions" : ["<all_urls>"],
    
    "browser_action" : {
        "default_popup" : "popup.html",
        "default_title" : "Wild West"
    }

    


}